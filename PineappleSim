local Section = TabThree:AddSection({
    Name = "Teleport"
})

   

    getgenv().TTPlace = "string"
    
    
    function Teleport(teleportPlace)
            local player = game.Players.LocalPlayer
            player.Character.HumanoidRootPart.CFrame = teleportPlace
                
    end

   
TabThree:AddDropdown({
	Name = "Select Zone",
	Default = "Candy",
	Options = {"Candy", "Snow", "Desert", "Volcano", "Forest"},
	Callback = function(Value)
		getgenv().TPPlace = Value
	end    
})

    TabThree:AddButton({
            Name = "Teleport",
            Callback = function(Value)
                if getgenv().TPPlace == "Candy" then
                    Teleport(game:GetService("Workspace").Doors.CandyZone.CFrame)
                elseif getgenv().TPPlace == "Snow" then
                    Teleport(game:GetService("Workspace").Doors.SnowZone.CFrame)
                elseif getgenv().TPPlace == "Desert" then
                    Teleport(game:GetService("Workspace").Doors.DesertZone.CFrame)
                elseif getgenv().TPPlace == "Volcano" then
                    Teleport(game:GetService("Workspace").Doors.DesertZone.CFrame)
                elseif getgenv().TPPlace == "Forest" then
                    Teleport(game:GetService("Workspace").Doors.ForestZone.CFrame)
                end
            end
    })


