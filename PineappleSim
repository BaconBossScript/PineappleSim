local Section = TabTwo:AddSection({
    Name = "Auto Hatch Fast"
})
    _G.autoHatch = true
    _G.selectEgg = "Normal"

    function autoHatch()
        while _G.autoHatch == true do
game:GetService("ReplicatedStorage").RemoteEvents.EggOpened:InvokeServer(_G.selectEgg, Single)

            wait(0)
        end
    end
TabTwo:AddToggle({
Name = "Auto Hatch",
    Default = false,
    Callback = function(Value)
                _G.autoHatch = Value
                    autoHatch()
                    end
        })

TabTwo:AddDropdown({
	Name = "Choose Eggs",
	Default = "Normal",
	Options = {"Normal", "Candy", "Forest", "Volcano"},
	Callback = function(Value)
		_G.selectEgg = Value
    print(_G.selectEgg)
	end    
})


